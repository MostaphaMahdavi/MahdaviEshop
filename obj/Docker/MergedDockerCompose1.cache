services:
  basketdb:
    container_name: basketdbmahdavi
    environment:
      name: value
    image: redis:alpine
    ports:
    - published: 6997
      target: 6379
    restart: always
  baskets.api:
    build:
      context: /Users/mostapha/Desktop/E/Test/MicroserviceProjects/MahdaviEshop
      dockerfile: Src/Services/Baskets/Baskets.Api/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Baskets.Api
      target: base
    container_name: Baskets.Api
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: baskets:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net6.0/Baskets.Api.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 5901
      target: 80
    tty: true
    volumes:
    - /Users/mostapha/Desktop/E/Test/MicroserviceProjects/MahdaviEshop/Src/Services/Baskets/Baskets.Api:/app:rw
    - /Users/mostapha/vsdbg/vs2017u5:/remote_debugger:rw
    - /usr/local/share/dotnet/sdk/NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - /Users/mostapha/.nuget/packages:/root/.nuget/packages:ro
    - /Users/mostapha/Desktop/E/Test/MicroserviceProjects/MahdaviEshop:/src:rw
  discountdb:
    container_name: discountdbmahdavi
    environment:
      POSTGRES_DB: DiscountDbMahdavi
