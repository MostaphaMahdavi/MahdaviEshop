services:
  basketdb:
    container_name: basketdbmahdavi
    environment:
      name: value
    image: redis:alpine
    ports:
    - published: 6997
      target: 6379
    restart: always
  baskets.api:
    build:
      context: /Users/mostapha/Desktop/E/Test/MicroserviceProjects/MahdaviEshop
      dockerfile: Src/Services/Baskets/Baskets.Api/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: baskets
    ports:
    - published: 5901
      target: 80
  discountdb:
    container_name: discountdbmahdavi
    environment:
      POSTGRES_DB: DiscountDbMahdavi
      POSTGRES_PASSWORD: admin1234
      POSTGRES_USER: admin
    image: postgres
    ports:
    - published: 6996
      target: 5432
    restart: always
    volumes:
    - postgres_data_discount:/var/lib/postgresql/data:rw
  discounts.api:
    build:
      context: /Users/mostapha/Desktop/E/Test/MicroserviceProjects/MahdaviEshop
      dockerfile: Src/Services/Discounts/Discounts.Api/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: discounts
    ports:
    - published: 5902
      target: 80
  loggingservices.api:
    build:
      context: /Users/mostapha/Desktop/E/Test/MicroserviceProjects/MahdaviEshop
      dockerfile: Src/Services/LoggingServices/LoggingServices.Api/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: loggingservices
    ports:
    - target: 80
  mongodb:
    container_name: mongodbmahdavi
    image: mongo
    ports:
    - published: 6963
      target: 27017
    restart: always
    volumes:
    - mongo-data:/data/db:rw
  pgadmin:
    container_name: pgadminmahdavi
    environment:
      PGADMIN_DEFAULT_EMAIL: admin@aspnetrun.com
      PGADMIN_DEFAULT_PASSWORD: admin1234
    image: dpage/pgadmin4
    ports:
    - published: 6998
      target: 80
    restart: always
    volumes:
    - pgadmin-data:/root/.pgadmin:rw
  productdb:
    container_name: productdbmahdavi
    environment:
      POSTGRES_DB: ProductDbMahdavi
      POSTGRES_PASSWORD: admin1234
      POSTGRES_USER: admin
    image: postgres
    ports:
    - published: 6999
      target: 5432
    restart: always
    volumes:
    - postgres_data:/var/lib/postgresql/data:rw
  products.api:
    build:
      context: /Users/mostapha/Desktop/E/Test/MicroserviceProjects/MahdaviEshop
      dockerfile: Src/Services/Products/Products.Api/Dockerfile
    depends_on:
      productdb:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: products
    ports:
    - published: 5900
      target: 80
  rabbitmq:
    container_name: rabbitmqmahdavi
    image: rabbitmq:3-management-alpine
    ports:
    - published: 6965
      target: 5672
    - published: 6964
      target: 15672
    restart: always
version: '3.4'
volumes:
  mongo-data: {}
  pgadmin-data: {}
  postgres_data: {}
  postgres_data_discount: {}